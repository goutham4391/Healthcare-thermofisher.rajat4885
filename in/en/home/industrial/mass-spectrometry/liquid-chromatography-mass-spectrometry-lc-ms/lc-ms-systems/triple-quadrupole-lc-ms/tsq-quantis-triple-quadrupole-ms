<script type="text/javascript">
var utilitybarExtended = (function ($this) {
	// Containers
	$this.$utilityBar = $('#user-utility-bar');

	// Utilities
	$this.$contactUs = $('#hfCustomerServiceLi');
	$this.$quickOrder = $('#hfBarQuickOrder');
	$this.$cloudLink = $('#hfCloud');
	$this.$miniCart = $('#miniCartButton');
	$this.$accountDropdown = $('#hfGreetByName, #hfGreetAnonymous');
	$this.$partnerDashboard = $('#hfGreetDashboard');
	$this.$viewNotifications = $('#notification-container');

	//Mobile Nav Drawers
	$this.$drawer1 = $('#mobile-nav-drawer-1');
	$this.$drawer2 = $('#mobile-nav-drawer-2');
	
	$this.$accountMenu = $('#hfGreetByName');
	$this.$signinMenu = $('#hfGreetAnonymous');
	
	$this.$accountDialog = $('#myaccount-div');

	$this.initCustomerName = function() {
		var lastFirstNameRegion = {'cn':'cn','kr':'kr','jp':'jp','tw':'tw'};
		var customerName = decode_utf8(_lt.user.getInfo('customerName'));
		var lastName = decode_utf8(_lt.user.getInfo('lastName'));
		var langCode = _lt.user.displayLang;
		var countryCode = getCookie('CK_ISO_CODE');
		if(countryCode!=='' && countryCode!=undefined && lastFirstNameRegion[countryCode]!=undefined && 
		lastFirstNameRegion[countryCode]!=='' && lastName!=='' && lastName!=undefined)
		{
			customerName = lastName+' '+customerName;
		}
		
		if (customerName && customerName.length) {
			if( langCode == 'ja' ) {
				$('#hfCustomerName').text(customerName+" "+localizedStrings.GREETING);
			}
			else {
				$('#hfCustomerName').text(customerName);
			}
			
			$('#hfCustomerNameLi').show();
		}
	};

	$this.init = function () {
		
		
		if ($this.$accountMenu.length) {
			$this.$accountMenu.append($this.$accountDialog);
		}
		else if ($this.$signinMenu.length) {
			$this.$signinMenu.append($this.$accountDialog);
		}
	};

    $this.assignMiniCartEventHandlers = function() {

    	var $miniCartContainer = $('#miniCartButton');
    	var $miniCartToggle = $('#viewMiniCart');

    	$miniCartContainer
    		.bind('mouseenter',function(e){
    			if ( !$miniCartContainer.hasClass('active') ) {
    				if (getCurrentViewportSize() =='sm' || getCurrentViewportSize() == 'xs'){
    					$(".content, .MainParsys, #pdp-content, .globalFooter, .footer").css("display", "none");
    				}
    			}
    			e.preventDefault();
    			return false;
    		})
    		.bind('mouseleave',function(){
    			if (getCurrentViewportSize() =='sm' || getCurrentViewportSize() == 'xs') {
    				$(".content, .MainParsys, #pdp-content, .globalFooter, .footer").css("display", "block");
    			}
    		});

    	$miniCartToggle
    		.bind('touchstart',function(e){
    			if ( !$miniCartContainer.hasClass('active') ) {
    				if (getCurrentViewportSize() =='sm' || getCurrentViewportSize() == 'xs' && $('#miniCartHeaderTotalItems').html() !== "" ){
    					$(".content, .MainParsys, #pdp-content, .globalFooter, .footer").css("display", "none");
    				}
    			} else {
    				hideOpenHeaderPanels();
    				if (getCurrentViewportSize() =='sm' || getCurrentViewportSize() == 'xs') {
    					$(".content, .MainParsys, #pdp-content, .globalFooter, .footer").css("display", "block");
    				}
    			}
    			e.preventDefault();
    			return false;
    		});
    };
	

	// DOM modifications for utilities
	$this.Large = function() {
		$this.$accountDropdown.prependTo($this.$utilityBar);
		$this.$partnerDashboard.prependTo($this.$utilityBar);
		if(!document.documentMode){
			$this.$viewNotifications.prependTo($this.$utilityBar);
		}
		$this.$miniCart.prependTo($this.$utilityBar);
		$this.$quickOrder.prependTo($this.$utilityBar);
	    $this.$contactUs.prependTo($this.$utilityBar);
		$this.$cloudLink.prependTo($this.$utilityBar);
		if($('.sticky-search').length !== 0) {
			$this.$cloudLink.addClass('hidden');
		} else {
			$this.$cloudLink.removeClass('hidden');
		}
	};

	$this.Medium = function() {
	    $this.$partnerDashboard.prependTo($this.$drawer2);
		$this.$accountDropdown.prependTo($this.$drawer2);
		if(!document.documentMode){
			$this.$viewNotifications.prependTo($this.$utilityBar);
		}
		$this.$miniCart.prependTo($this.$utilityBar);
		$this.$quickOrder.prependTo($this.$utilityBar);
		$this.$contactUs.prependTo($this.$drawer2);
		$this.$cloudLink.remove();
	};

	$this.Small = function() {
	    $this.$partnerDashboard.prependTo($this.$drawer2);
		$this.$accountDropdown.prependTo($this.$drawer2);
		if(!document.documentMode){
			$this.$viewNotifications.prependTo($this.$utilityBar);
		}
		$this.$miniCart.prependTo($this.$utilityBar);
		$this.$quickOrder.prependTo($this.$drawer1);
		$this.$contactUs.prependTo($this.$drawer2);
		$this.$cloudLink.remove();
	};

	$this.triggerViewportChange = function() {
		currentBreakpoint = getCurrentViewportSize();
		switch ( currentBreakpoint ) {
			case "xl":
			case "lg":
				$this.Large();
				break;
			case "md":
			case "sm":		
			    $this.Medium();
				break;
			case "xs":
				$this.Small();
				break;
		}
	};

	$this.initCustomerName();
	$this.assignMiniCartEventHandlers();
	$this.init();
	$(window).bind('resize', $this.triggerViewportChange);
	return $this;
}(utilitybarExtended || {}));

// Loyalty menu item
$(document).ready(function() {
    if(document.location.href.indexOf('loginDisplay') == -1 && document.location.href.indexOf('&regType=SingleStepFullReg') == -1) {
        localStorage.setItem('aspireFormSignIn', 2);
     }
     if ( _lt.user.loyalty.isUserEnrolled() ) {
         _lt.user.loyalty.userUrl = _lt.user.loyalty.dashboardPageUrl;
         $('#loyaltyPoints').text( _lt.user.loyalty.points + " points" );
     } else if(_lt.user.loyalty.isUserNotEnrolled()){
         // if user is not enrolled direct them to Loyalty info page
         _lt.user.loyalty.userUrl = _lt.user.loyalty.landingPageUrl;
         _lt.user.loyalty.points = "A free program to achieve your highest scientific aspirations";
         $('#loyaltyPoints').text( _lt.user.loyalty.points);
     }
     if ( _lt.user.loyalty.isUserActive() && _lt.user.loyalty.show ) {
         $('#hfLoyalty').css("display", "block");
         $("#loyaltyPoints").css("display", "block");
         $('#loyaltyLink').attr('href',_lt.user.loyalty.userUrl);
         $('#loyaltyLinkText').html(_lt.user.loyalty.userUrlText);
     }
});

</script>


</div>


























































