<script type="text/javascript">
var utilitybarBase = (function ($this) {
	// Containers
	$this.$utilityBar = document.getElementById('user-utility-bar');
	
	// Utilities
	$this.$quickOrder = document.getElementById('hfBarQuickOrder');
	$this.$cloudLink = document.getElementById('hfCloud');
	$this.$accountDropdown = document.querySelectorAll('#hfGreetAnonymous,#hfGreetByName')[0];
	$this.$partnerDashboard = document.querySelectorAll('#hfGreetDashboard')[0];
	$this.$miniCart = document.getElementById('miniCartButton');
	$this.$viewNotifications = document.getElementById('notification-container');

	//Mobile Nav Drawers
	$this.$drawer1 = document.getElementById('mobile-nav-drawer-1');
	$this.$drawer2 = document.getElementById('mobile-nav-drawer-2');

	// DOM modifications for utilities
	$this.Large = function () {
	    if (!!$this.$partnerDashboard) {
	        $this.$utilityBar.appendChild($this.$partnerDashboard);
	    }
		if (!!$this.$cloudLink) {
		    $this.$utilityBar.appendChild($this.$cloudLink);
		}
		if (!!$this.$quickOrder) {
		    $this.$utilityBar.appendChild($this.$quickOrder);
		}
		if (!!$this.$miniCart) {
		    $this.$utilityBar.appendChild($this.$miniCart);
		}
		if(!!$this.$viewNotifications && !document.documentMode){
			$this.$utilityBar.appendChild($this.$viewNotifications);
		}
		if (!!$this.$accountDropdown) {
		    $this.$utilityBar.appendChild($this.$accountDropdown);
		}
	};

	$this.Medium = function () {
		if (!!$this.$accountDropdown) {
		    $this.$drawer2.appendChild($this.$accountDropdown);
		}
		if (!!$this.$partnerDashboard) {
		    $this.$drawer2.appendChild($this.$partnerDashboard);
		}
		if (!!$this.$quickOrder) {
		    $this.$utilityBar.appendChild($this.$quickOrder);
		}
		if (!!$this.$miniCart) {
		    $this.$utilityBar.appendChild($this.$miniCart);
		}
		if(!!$this.$viewNotifications && !document.documentMode){
			$this.$utilityBar.appendChild($this.$viewNotifications);
		}
	};

	$this.Small = function () {
		if (!!$this.$accountDropdown) {
		    $this.$drawer2.appendChild($this.$accountDropdown);
		}
		if (!!$this.$partnerDashboard) {
		    $this.$drawer2.appendChild($this.$partnerDashboard);
		}
		if (!!$this.$miniCart) {
		    $this.$utilityBar.appendChild($this.$miniCart);
		}
		if(!!$this.$viewNotifications && !document.documentMode){
			$this.$utilityBar.appendChild($this.$viewNotifications);
		}
		if (!!$this.$quickOrder) {
		    $this.$drawer1.appendChild($this.$quickOrder);
		}
	};

	$this.init = function() {
		currentBreakpoint = getCurrentViewportSize();
		switch ( currentBreakpoint ) {
			case "xl":
			case "lg":
				$this.Large();
				break;
			case "md":
			case "sm":			
			    $this.Medium();
				break;
			case "xs":
				$this.Small();
				break;
		}
	};

    $this.initMiniCartCount = function() {
	    var cartitemsClassname = '.minicart-number-holder-' + (window.innerWidth <= 979 ? 'mobile' : 'desktop');
    	// returns single element based on viewport (screen width)
	    var $headerItemCount = document.querySelector(cartitemsClassname);
    	// null check for headerItermCount
    	var $miniCartIcon = $headerItemCount != null ? $headerItemCount.parentNode.querySelector('.icon')  : null;
    	var CART_FULL_COUNT = 9999;

    	var itemCount = getCookie('MiniCartTotalItems');
        // null check for minicart element
    	if (!!itemCount && itemCount > 0 && $miniCartIcon) {

    		// If we have items, set it and remove empty style
    		$miniCartIcon.classList.remove('cart-icon-empty');
			$headerItemCount.classList.remove('hidden');
			$headerItemCount.classList.add('cart-items-number');

    		// Too full? Use 999 as item count
    		if ( itemCount > CART_FULL_COUNT ) {
    			$headerItemCount.innerHTML = CART_FULL_COUNT;
    			$miniCartIcon.classList.add('cart-icon-full');
				$headerItemCount.classList.add('cart-items-number');
				$headerItemCount.classList.remove('hidden');

    		} else {
    			$headerItemCount.innerHTML = itemCount;
    			$miniCartIcon.classList.remove('cart-icon-full');
    		}

    	}
    };

	// Including script to initialize notification component
	if(!document.documentMode && $this.$viewNotifications ){
        loadScript('../../../../../../../../store/v2/web-components/vendor.js');
        loadScript('../../../../../../../../store/v2/web-components/react-web-components.js');
    }

    function loadScript(url){    
        var body = document.body;
        var script = document.createElement('script');
        script.type = 'text/javascript';
		script.defer = true;
        script.src = url;
        body.appendChild(script);
	}

	$this.init();
	$this.initMiniCartCount();	
	return $this;
}(utilitybarBase || {}));

</script>

</div>

                    </div>

