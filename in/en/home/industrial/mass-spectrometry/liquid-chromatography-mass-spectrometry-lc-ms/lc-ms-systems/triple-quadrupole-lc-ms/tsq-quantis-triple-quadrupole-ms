</style><!-- Menu Nav JS Start --><script>
jQuery(document).ready(function(){

$(function () {
  var sticky_navigation_offset_top;
  var sticky_nav_off_top = $('.sticky');

  if (sticky_nav_off_top.length) {
    sticky_navigation_offset_top = sticky_nav_off_top.offset().top;
  }

  var sticky_navigation = function () {
    var scroll_top = $(window).scrollTop();

        if (scroll_top > sticky_navigation_offset_top - 100 ) {
          $('.sticky').addClass('fixed');
        } 
        else {
          $('.sticky').removeClass('fixed');
        }
  };

  sticky_navigation();
  $(window).scroll(function () {
    sticky_navigation();
  });
});

  $( "#jump-links-nav1" ).mouseover(function() {
  $( "#jump-links-nav5, #jump-links-nav4, #jump-links-nav2, #jump-links-nav3" ).removeClass( "active-nav" );
  });
  $("#jump-links-nav1").click(function(){
  $('#jump-links-nav2, #jump-links-nav5, #jump-links-nav4, #jump-links-nav3, #jump-links-nav6').removeClass('active-nav');
  $('#jump-links-nav1').addClass('active-nav');
  });
  $("#jump-links-nav2").click(function(){
  $('#jump-links-nav4, #jump-links-nav5, #jump-links-nav1, #jump-links-nav3, #jump-links-nav6').removeClass('active-nav');
  $('#jump-links-nav2').addClass('active-nav');
  });
  $("#jump-links-nav3").click(function(){
  $('#jump-links-nav5, #jump-links-nav2, #jump-links-nav1, #jump-links-nav4, #jump-links-nav6').removeClass('active-nav');
  $('#jump-links-nav3').addClass('active-nav');
  });
  $("#jump-links-nav4").click(function(){
  $('#jump-links-nav5, #jump-links-nav2, #jump-links-nav1, #jump-links-nav3, #jump-links-nav6').removeClass('active-nav');
  $('#jump-links-nav4').addClass('active-nav');
  });
  $("#jump-links-nav5").click(function(){
  $('#jump-links-nav4, #jump-links-nav2, #jump-links-nav1, #jump-links-nav3, #jump-links-nav6').removeClass('active-nav');
  $('#jump-links-nav5').addClass('active-nav');
  });
