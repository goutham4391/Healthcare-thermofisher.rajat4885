<script id="minicartScript" src="../../../../../../../../store/minicartjs/scripts/minicart.min.js"></script>


<script id="identityGlobal" src="../../../../../../../../account-center/scripts/identityGlobal.min.js"></script>


<script type="text/javascript">
!function($){var secLinks=document.querySelectorAll("a[href^\x3d'https://sec-assets']");for(var i=0;i<secLinks.length;i++){secLinks[i].setAttribute("target","_blank");secLinks[i].addEventListener("click",function(e){e.preventDefault();validateSecureAssetsSignIn(e,$(this))})}function validateSecureAssetsSignIn(evt,$this){storageAssets=function(){var storageDomain;var isOnStorageDomain;var iframe;var eventsPending=[];var eventsQueue={};var status=0;var body=document.getElementsByTagName("body")[0];
function init(){if(status===0){if(!window.environmentURL)return false;storageDomain="https://"+window.location.host;isOnStorageDomain=true;if(!isOnStorageDomain)initIframe();else{status=1;processPendingRequests()}}}function initIframe(){status=5;iframe=document.createElement("iframe");iframe.src=storageDomain+"/shared-static/support/_lt.localStorage.html";iframe.style.display="none";iframe.onload=function(){status=1;processPendingRequests()};iframe.onerror=function(){status=9};body.appendChild(iframe);
if(window.attachEvent)window.attachEvent("message",receiveMessage);else window.addEventListener("message",receiveMessage,false)}function receiveMessage(evt){if(evt&&evt.data&&typeof evt.data=="string"){var data=evt.data.split(":");var origin=evt.origin;var action=decodeURIComponent(data[0]);var id=decodeURIComponent(data[1]);var key=decodeURIComponent(data[2]);var payload=decodeURIComponent(data[3]);if(origin.indexOf("thermofisher.")!==-1)processAndTransmitPayload(payload,eventsQueue[action+":"+id])}}
function processAndTransmitPayload(payload,callback){if(!payload)payload=null;else if(payload==="lsFailure")payload=false;if(callback)callback(payload)}function processPendingRequests(){var event;for(var i=0;i<eventsPending.length;i++){event=eventsPending[i];processRequest(event.action,event.key,event.value,event.callback)}}function processRequest(action,key,value,callback){init();if(status!==1)eventsPending.push({action:action,key:key,value:value,callback:callback});else if(!isOnStorageDomain){var requestId=
Math.floor(Math.random()*1E6+1);var transmitKey="ls-"+action+":"+requestId;var transmitPayload=transmitKey;if(key){transmitPayload+=":"+encodeURIComponent(key);if(value)transmitPayload+=":"+encodeURIComponent(value)}eventsQueue[transmitKey]=callback;iframe.contentWindow.postMessage(transmitPayload,storageDomain)}else{var out=null;try{if(key&&value)out=window.localStorage[action](key,value);else if(key)out=window.localStorage[action](key);else out=window.localStorage[action]()}catch(e){out="lsFailure"}processAndTransmitPayload(out,
callback)}}var storage={getItem:function(key,cb){processRequest("getItem",key,null,cb)},setItem:function(key,value,cb){processRequest("setItem",key,value,cb)}};return storage}();var hostName=window.location.hostname;var contentUrl="/"+_lt.user.displayIso+"/"+_lt.user.displayLang+"/syndicated-content/snippets/terms-secure-file.html";var domainXtn=hostName.split("thermofisher.")[1];var assetsPath=$this.attr("href").split(domainXtn+"/")[1];var restURL="https://"+hostName+"/api-gateway/tfdocs/signedurl"+
"?objectKey\x3d"+assetsPath;var acceptanceKey="secureFileAccepted";var textTitle=localizedStrings.ACCEPTANCE||"Acceptance to Terms of Use";var textAgree=localizedStrings.BTN_AGREE||"Agree";var textDisAgree=localizedStrings.BTN_DISAGREE||"Disagree";var textError=localizedStrings.SUBMIT_ERROR_SERVER||"Error - please try again.";var respObject;var preSignedURL;storageAssets.getItem(acceptanceKey,function(response){if(_lt.user.isLoggedIn){$.ajax({type:"GET",url:restURL,dataType:"text",cache:false,async:false,
success:function(resp){try{respObject=JSON.parse(resp)}catch(e){console.log("Error in Parsing Json File")}},error:function(xhr,status,error){},complete:function(){if(respObject!=null&&typeof respObject!="undefined")preSignedURL=respObject.success.preSignedURL;else console.log("Error in getting presigned URL")}});if(response!=null&&response=="Y"){window.open(preSignedURL,"_blank");_lt.user.initLoginChecks()}else{var $modal=$('\x3cdiv class\x3d"modal hide modal-loading modal-secure-file-acceptance"\x3e'+
'\x3cdiv class\x3d"modal-header"\x3e'+'\x3cbutton type\x3d"button" class\x3d"close" data-dismiss\x3d"modal" aria-hidden\x3d"true"\x3e\x26times;\x3c/button\x3e'+"\x3ch3\x3e"+textTitle+"\x3c/h3\x3e"+"\x3c/div\x3e"+'\x3cdiv class\x3d"modal-body"\x3e\x3cdiv class\x3d"text-center"\x3e\x3cspan class\x3d"loading-wheel loading-wheel-md"\x3e\x3c/span\x3e\x3c/div\x3e\x3c/div\x3e'+'\x3cdiv class\x3d"modal-footer"\x3e'+'\x3cbutton type\x3d"button" class\x3d"btn btn-disagree" data-dismiss\x3d"modal"\x3e'+textDisAgree+
"\x3c/button\x3e"+'\x3ca href\x3d"'+preSignedURL+'" target\x3d"_blank" class\x3d"btn btn-primary btn-agree"\x3e'+textAgree+"\x3c/a\x3e"+"\x3c/div\x3e"+"\x3c/div\x3e");var modalBody=$(".modal-body",$modal);$(".btn-disagree",$modal);$(".btn-agree",$modal).click(function(){storageAssets.setItem(acceptanceKey,"Y");$modal.modal("hide");$modal.remove();_lt.user.initLoginChecks()});$modal.on("hidden",function(){$modal.remove()});$.get(contentUrl).success(function(resp){modalBody.html(resp);$modal.removeClass("modal-loading")}).error(function(resp){modalBody.text(textError)});
$modal.appendTo("body");$modal.modal({backdrop:"static",keyboard:false});$modal.modal("show");$("body").append("\x3cstyle\x3e.modal-secure-file-acceptance.modal-loading .modal-footer { display:none; }\x3c/style\x3e");evt.stopImmediatePropagation()}}else invokeOidcLogin(window.location.href)})}}(window.$j);
