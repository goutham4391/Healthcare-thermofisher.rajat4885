<script type="text/javascript">
var meganavBase = (function ($this) {

	$this.updateTitleText = function () {
		var newViewport = getCurrentViewportSize();
		var titles = document.getElementById("meganav-container").getElementsByTagName('span[data-title-default]');
		for (var i = 0; i < titles.length; i++) {
			console.log(titles[i]);
			var titletext = titles[i].getAttribute("data-title-" + newViewport);

			if (titletext) {
				titles[i].querySelector('b') !== null ?
					titles[i].querySelector('b').text(titletext) :
					titles[i].text(titletext);
			} else {
				titles[i].querySelector('b') !== null ?
					titles[i].querySelector('b').text(titles[i].getAttribute('data-title-default')) :
					titles[i].text(item.getAttribute('data-title-default'))
			}
		}
	}

	$this.show = function() {
		document.getElementById('meganav-content').style.display = 'block';
	}
	
	$this.updateTitleText();
	$this.show();
	
	return $this;
}(meganavBase || {}));

/*Variables for the meganav delay and display of opacity */
meganavnamespace={};
meganavnamespace.bLeftTheMenu=true;
meganavnamespace.meganavDisplayDelay=450;
meganavnamespace.meganavOpacityDelay=450;
function initMeganavHoverHandlers(){
	// Sometimes authors place custom jQuery versions down that override existing plugins
	// This wont fix the page but will prevent JS error
	if ( !!$.fn.hoverIntent ) {
		// hoverIntent allows for 150ms delay in hide/show
		$('.meganav > li').hoverIntent({
			over: function showMenu() {
				$(this).addClass('active').addClass('in');
				var $jq = (typeof $j == "function") ? $j : $;
				$jq('.searchAutoCompleteProviderSelector').trigger('chosen:close');
			},
			timeout: 150,
			out: function hideMenu() {
				$(this).removeClass('active').removeClass('in');
			}
		});
	}
}
</script>

</div>

